{
  "name": "node-ts-template",
  "version": "1.0.0",
  "description": "Node TS project setup template",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup src",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsup src --watch\" \"nodemon dist/index.js\"",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamraviteja/Node-TS-Template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iamraviteja/Node-TS-Template/issues"
  },
  "homepage": "https://github.com/iamraviteja/Node-TS-Template#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
