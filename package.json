{
  "name": "node-ts-template",
  "version": "1.0.0",
  "description": "Node TS project setup template",
  "main": "dist/index.js",
  "scripts": {
    "build:server": "etsc --config=etsc.config.build.js",
    "build:client": "esbuild src/client/index.tsx --bundle --outfile=dist/public/bundle.js",
    "start": "node dist/server/index.js",
    "dev": "nodemon",
    "generate-gql": "graphql-codegen",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamraviteja/Node-TS-Template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iamraviteja/Node-TS-Template/issues"
  },
  "homepage": "https://github.com/iamraviteja/Node-TS-Template#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^18.11.18",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "cpy": "^10.1.0",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "rimraf": "^5.0.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@apollo/client": "^3.8.4",
    "@apollo/server": "^4.9.3",
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "esbuild": "^0.19.3",
    "esbuild-node-tsc": "^2.0.5",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
